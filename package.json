{
  "name": "spotify-lambda",
  "version": "0.1.0",
  "bin": {
    "spotify-lambda": "bin/spotify-lambda.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "cp package.json dist/package.json && cd dist && npm install --only=production",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "prepare": "husky install",
    "static-analysis": "npx eslint . --ext .ts",
    "prettier": "npx prettier --write .",
    "package-update": "npx ncu -u"
  },
  "dependencies": {
    "axios": "0.25.0"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.17.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.17.0-alpha.0",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "aws-cdk-lib": "2.17.0",
    "constructs": "10.0.91",
    "eslint": "8.9.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "npm-check-updates": "12.3.0",
    "prettier": "2.5.1",
    "source-map-support": "0.5.21",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "*": "npm run prettier"
  }
}
