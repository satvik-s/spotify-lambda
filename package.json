{
  "name": "spotify-lambda",
  "version": "0.1.0",
  "bin": {
    "spotify-lambda": "bin/spotify-lambda.js"
  },
  "scripts": {
    "build-now-playing": "$(npm bin)/tsc --project tsconfig.now-playing.json",
    "build-top-tracks": "$(npm bin)/tsc --project tsconfig.top-tracks.json",
    "postbuild-now-playing": "cp package.json dist-now-playing/package.json && cd dist-now-playing && npm install --only=production",
    "postbuild-top-tracks": "cp package.json dist-top-tracks/package.json && cd dist-top-tracks && npm install --only=production",
    "watch": "$(npm bin)/tsc -w",
    "test": "$(npm bin)/jest",
    "prepare": "$(npm bin)/husky install",
    "static-analysis": "$(npm bin)/eslint .",
    "prettier": "$(npm bin)/prettier --write .",
    "check-updates": "$(npm bin)/ncu",
    "make-updates": "$(npm bin)/ncu -u"
  },
  "dependencies": {
    "axios": "0.27.2"
  },
  "devDependencies": {
    "@types/jest": "27.5.1",
    "@types/node": "17.0.33",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "aws-cdk-lib": "2.23.0",
    "constructs": "10.1.7",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "lint-staged": "12.4.1",
    "npm-check-updates": "12.5.11",
    "prettier": "2.6.2",
    "source-map-support": "0.5.21",
    "ts-jest": "28.0.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  },
  "lint-staged": {
    "*{ts,js,json}": "npm run prettier"
  }
}
