{
  "name": "spotify-lambda",
  "version": "0.1.0",
  "bin": {
    "spotify-lambda": "bin/spotify-lambda.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "cp package.json dist/package.json && cd dist && npm install --only=production",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "prepare": "husky install",
    "static-analysis": "npx eslint . --ext .ts",
    "prettier": "npx prettier --write ."
  },
  "dependencies": {
    "axios": "0.25.0"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.8.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.8.0-alpha.0",
    "@types/jest": "26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "aws-cdk-lib": "2.8.0",
    "constructs": "10.0.0",
    "eslint": "8.7.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "husky": "7.0.4",
    "jest": "26.4.2",
    "lint-staged": "12.3.1",
    "prettier": "2.5.1",
    "source-map-support": "0.5.16",
    "ts-jest": "26.2.0",
    "ts-node": "9.0.0",
    "typescript": "3.9.7"
  },
  "lint-staged": {
    "*": "npm run prettier"
  }
}
