{
  "name": "spotify-lambda",
  "version": "0.1.0",
  "bin": {
    "spotify-lambda": "bin/spotify-lambda.js"
  },
  "scripts": {
    "build": "$(npm bin)/tsc",
    "postbuild": "cp package.json dist/package.json && cd dist && npm install --only=production",
    "watch": "$(npm bin)/tsc -w",
    "test": "$(npm bin)/jest",
    "prepare": "$(npm bin)/husky install",
    "static-analysis": "$(npm bin)/eslint .",
    "prettier": "$(npm bin)/prettier --write .",
    "check-updates": "$(npm bin)/ncu",
    "make-updates": "$(npm bin)/ncu -u"
  },
  "dependencies": {
    "axios": "0.26.1"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.29",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "aws-cdk-lib": "2.21.1",
    "constructs": "10.0.124",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.4.1",
    "npm-check-updates": "12.5.10",
    "prettier": "2.6.2",
    "source-map-support": "0.5.21",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "lint-staged": {
    "*{ts,js,json}": "npm run prettier"
  }
}
